    <script>
        let ham = document.getElementById("ham")
            let x = document.querySelector(".navar")

            document.getElementById("ham").addEventListener("click", (e) =>{
                if (x.className === "navbar") {
                    x.className += " responsive";
                } else {
                    x.className = "navbar"
                }
            })
